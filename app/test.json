{
   "collectCoverage":true,
   "coverageThreshold":{
      "global":{
         "branches":85,
         "functions":85,
         "lines":85,
         "statements":85
      }
   },
   "collectCoverageFrom":[
      "**/*.js",
      "!build/**/*.js",
      "!coverage/**/*.js"
   ],
   "testMatch":[
      "**/?(*.)(spec).js?(x)"
   ],
   "testPathIgnorePatterns":[
      "<rootDir>/build",
      "<rootDir>/node_modules"
   ]
}